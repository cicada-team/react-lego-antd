{
  "name": "@alipay/cicada",
  "description": "do not use this package",
  "version": "0.0.1",
  "scripts": {
    "installAll": "tnpm i && cd ./components && tnpm i && cd ..",
    "start": "next",
    "test": "jest",
    "build": "rm -rf lib && babel src --out-dir lib && lessc src/index.less lib/index.css",
    "prepublish": "npm run build",
    "coverage": "jest --coverage",
    "lint": "eslint render components examples",
    "eslint-fix": "eslint--fix render components examples  --ext '.js,.jsx' && eslint-tinker ./examples/**/*.md",
    "commit": "./scripts/commit.js commit",
    "build-doc": "next build",
    "update-doc": "./scripts/doc.js && git add doc/",
    "help": "./scripts/commit.js"
  },
  "devDependencies": {
    "next": "^3.0.0-beta13",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "dedent": "^0.7.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "jsdom-global": "^2.1.1",
    "json-stable-stringify": "^1.0.1",
    "pre-commit": "^1.1.3",
    "prettyjson": "^1.1.3",
    "react-document-title": "^2.0.2",
    "react-keydown": "^1.6.2",
    "react-panelgroup": "^1.0.1",
    "react-router": "^2.7.0",
    "simple-git": "^1.65.0",
    "svg-sprite-loader": "^0.3.0",
    "jest-cli": "^14.1.0",
    "less": "^2.7.1",
    "react-addons-test-utils": "^15.3.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "pre-commit": [
    "lint"
  ],
  "tnpm": {
    "mode": "npminstall"
  },
  "dependencies": {
    "@cicada/render": "^1.0.0-alpha3",
    "antd": "^2.11.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "classnames": "^2.2.5",
    "moment": "^2.15.1",
    "prop-types": "^15.5.10",
    "rc-animate": "^2.3.1",
    "rc-dialog": "^6.4.2",
    "rc-pagination": "^1.5.5",
    "rc-table": "^5.0.3",
    "rc-util": "^4.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/*",
      "react",
      "react-dom",
      "react-addons-test-utils"
    ],
    "modulePathIgnorePatterns": [
      "/_site/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/webpack-babel-jest",
    "testDirectoryName": "test"
  }
}
